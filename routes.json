{
  "routes": [
    {
      "route": "/api/v1/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "backendUri": "https://api.propease.eu/api/v1/*"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/index.html"
    }
  ]
}
